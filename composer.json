{
  "name": "daikazu/laratone",
  "description": "Color Libraries for Laravel",
    "authors": [
    {
      "name": "Mike Wall",
      "email": "daikazu@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "5.6.*|5.7.*"
