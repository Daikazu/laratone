{
  "name": "daikazu/laratone",
  "description": "Color Libraries and API for Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "swatches",
    "laratone"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Mike Wall",
      "email": "daikazu@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": ">=6.0"
  },
  "require-dev": {
    "orchestra/testbench": ">=4.0",
    "phpunit/phpunit": ">=8.4"
  },
  "autoload": {
    "psr-4": {
      "Daikazu\\Laratone\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Daikazu\\Laratone\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Daikazu\\Laratone\\ServiceProvider"
      ],
      "aliases": {
        "Laratone": "Daikazu\\Laratone\\Laratone"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
