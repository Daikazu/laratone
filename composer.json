{
  "name": "daikazu/laratone",
  "description": "Color Library for Laravel",
  "version": "v0.0.2",
  "authors": [
    {
      "name": "Mike Wall",
      "email": "daikazu@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.5.5",
    "illuminate/support": "5.5.*|5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.0.*"
  },
  "autoload": {
    "psr-4": {
      "Daikazu\\Laratone\\": "src/",
      "Daikazu\\Laratone\\Colorbooks\\": "src/seeds/"
    }
  },
  "minimum-stability": "RC",
  "extra": {
    "laravel": {
      "providers": [
        "Daikazu\\Laratone\\LaratoneServiceProvider"
      ]
    }
  }
}
